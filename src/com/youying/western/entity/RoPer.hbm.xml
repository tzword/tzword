<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.youying.western.entity.RoPer" table="T_RO_PER">
        <id name="id" type="java.lang.Integer">
			<column name="id" />
			<generator class="sequence">
				<param name="sequence">SEQ_RO_PER</param>
			</generator>
		</id>
        <property name="roleId" type="java.lang.Integer">
            <column name="ROLE_ID" />
        </property>
        <property name="permissionId" type="java.lang.Integer">
            <column name="PERMISSION_ID" />
        </property>
        <property name="createDt" type="java.util.Date">
            <column name="CREATE_DT" />
        </property>
    </class>
</hibernate-mapping>
